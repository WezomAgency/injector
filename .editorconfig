# http://EditorConfig.org
root = true
charset = utf-8
indent_style = space
indent_size = tab
trim_trailing_whitespace = true
end_of_line = crlf
insert_final_new_line = true

[*.md]
trim_trailing_whitespace = false
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 2

[*.yml]
indent_style = space
indent_size = 2
